<script>
    (function(){

        //move to register
        var toRegister = function(){
            document.getElementById("form_warn").style.display = "none";
            document.getElementById("form2").style.display = "none";
            document.getElementById("form1").style.display = "block";
        }
        //move to Login
        var toLogin = function(){
            document.getElementById("form_warn").style.display = "none";
            document.getElementById("form1").style.display = "none";
            document.getElementById("form2").style.display = "block";
        }
        //add listeners to buttons
        window.addEventListener('DOMContentLoaded', function () {
            document.getElementById("reg").addEventListener('click', function(){toRegister();});
            document.getElementById("log").addEventListener('click', function(){toLogin();});
        });

    })();

</script>
<style>
    #log, #reg{
        color: blue;
        cursor: pointer;
    }
</style>
<title>Login</title>
</head>